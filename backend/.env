# Weapons Detection Configuration Template
# Copy this content to a .env file and fill in your actual values

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash:generateContent

# Detection Settings
CONFIDENCE_THRESHOLD=0.5
MIN_TIME_BETWEEN_SHOTS=2
EVENT_GROUPING_DURATION=60

# Folder Settings
SCREENSHOT_FOLDER=weapon_detections
EVENTS_FOLDER=weapon_events

# Model Settings
MODEL_PATH=best_fine-tuned_model.pt
